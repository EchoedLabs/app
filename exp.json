{
  "name": "echoed-labs-app",
  "description": "A React Native app for Echoed Labs.",
  "slug": "elapp",
  "privacy": "unlisted",
  "sdkVersion": "12.0.0",
  "version": "1.0.0",
  "orientation": "portrait",
  "primaryColor": "#6ea9c7",
  "iconUrl": "https://echoedlabs.com/wp-content/uploads/2017/01/RGB-Echoed.png",
  "androidStatusBarColor": "#6ea9c7",
  "notification": {
    "iconUrl": "https://echoedlabs.com/wp-content/uploads/2016/02/RGB-Echoed-48x48.png",
    "color": "#6ea9c7",
    "androidMode": "collapse",
    "androidCollapsedTitle": "#{unread_notifications} updates"
  },
  "loading": {
    "iconUrl": "https://echoedlabs.com/wp-content/uploads/2017/01/RGB-Echoed.png",
    "hideExponentText": true
